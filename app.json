{
  "expo": {
    "name": "BiLiraTest",
    "slug": "BiLiraTest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font", 
        {
          "fonts": ["./assets/fonts/Archivo-Bold.ttf", "./assets/fonts/Archivo-ExtraBold.ttf", "./assets/fonts/Archivo-ExtraLight.ttf", "./assets/fonts/Archivo-Light.ttf", "./assets/fonts/Archivo-Medium.ttf", "./assets/fonts/Archivo-SemiBold.ttf", "./assets/fonts/Archivo-Regular.ttf", "./assets/fonts/Archivo-Thin.ttf"]
        }
      ]
    ]
  }
}
